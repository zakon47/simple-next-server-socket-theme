  "scripts": {
    "dev:server": "cross-env NODE_SERV=two_server nodemon --exec ts-node --project tsconfig.server.json server/index.ts",
    "dev:next": "cross-env NODE_SERV=two_server next dev -p 3100",
    "dev:full": "nodemon --exec ts-node --project tsconfig.server.json server/index.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:next\" ",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "cross-env NODE_ENV=production HOST1=http://localhost:3300 node dist/index.js",
    "prod": "concurrently --restart-tries=100 \"npm run start\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
